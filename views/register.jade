doctype html
html.coming-soon(lang='pt-BR')
  head
    meta(charset='utf-8')
    title Registration Form
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no')
    meta(name='apple-mobile-web-app-capable', content='yes')
    meta(name='apple-touch-fullscreen', content='yes')
    meta(name='author', content='KaijuThemes')
    link(href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400italic,600', rel='stylesheet', type='text/css')
    link(type='text/css', href='assets/plugins/iCheck/skins/minimal/blue.css', rel='stylesheet')
    link(type='text/css', href='assets/fonts/font-awesome/css/font-awesome.min.css', rel='stylesheet')
    link(type='text/css', href='assets/fonts/themify-icons/themify-icons.css', rel='stylesheet')
    // Themify Icons
    link(type='text/css', href='assets/css/styles.css', rel='stylesheet')
    // HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries. Placeholdr.js enables the placeholder attribute
    //if lt IE 9
      link(type='text/css', href='assets/css/ie8.css', rel='stylesheet')
      script(type='text/javascript', src='http://html5shim.googlecode.com/svn/trunk/html5.js')
    // The following CSS are included as plugins and can be removed if unused
  body.focused-form.animated-content
    #registration-form.container
      a.login-logo(href='index.html')
        img(src='assets/img/logo-big.png' width='50px')
      .row
        .col-md-6.col-md-offset-3
          .panel.panel-default
            .panel-heading
              h2 Registration Form
            .panel-body
              form.form-horizontal(action='')
                .form-group.mb-md
                  label.col-xs-4.control-label(for='FullName') Full Name
                  .col-xs-8
                    input#FulltName.form-control(type='text', name='FullName', placeholder='Full Name', required='')
                .form-group.mb-md
                  label.col-xs-4.control-label(for='Username') Username
                  .col-xs-8
                    input#Username.form-control(type='text', name='Username', placeholder='Username', required='')
                .form-group.mb-md
                  label.col-xs-4.control-label(for='Email') Email
                  .col-xs-8
                    input#Email.form-control(type='text', name='Email', placeholder='Email', required='')
                .form-group.mb-md
                  label.col-xs-4.control-label(for='Password') Password
                  .col-xs-8
                    input#Password.form-control(type='password', name='Password', placeholder='Password', required='')
                .form-group.mb-md
                  label.col-xs-4.control-label(for='ConfirmPassword') Confirm
                  .col-xs-8
                    input#ConfirmPassword.form-control(type='password', name='ConfirmPassword', placeholder='Confirm Password', required='')
                .form-group.mb-n
                  .col-xs-12
                    .checkbox.icheck
                      label(for='')
                        input(type='checkbox')
                        |  I accept the 
                        a(href='#') user agreement
            .panel-footer
              .clearfix
                a.btn.btn-default.pull-left(href='extras-login.html') Already Registered? Login
                a.btn.btn-primary.pull-right(href='extras-registration.html') Register
